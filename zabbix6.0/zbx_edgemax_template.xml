                    <uuid>36f64bd4b35d41d9a8fde299df72ca9a</uuid>
                    <name>Network interfaces discovery</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#IFOPERSTATUS},1.3.6.1.2.1.2.2.1.8,{#IFNAME},1.3.6.1.2.1.31.1.1.1.1]</snmp_oid>
